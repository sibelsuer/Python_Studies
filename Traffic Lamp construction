from turtle import *
shape("blank")
pensize(6)

fillcolor("black")
begin_fill()
for x in range(2):
    forward(100)
    right(90)
    forward(250)
    right(90)
end_fill()

pencolor("red")
penup()
goto(50,-80)
pendown()
fillcolor("red")
begin_fill()
circle(30)
end_fill()

pencolor("yellow")
penup()
goto(50,-150)
pendown()
fillcolor("yellow")
begin_fill()
circle(30)
end_fill()

pencolor("green")
penup()
goto(50,-220)
pendown()
fillcolor("green")
begin_fill()
circle(30)
end_fill()

